pdataMenu.data === \"이미 동일한 이름이 존재합니다.\") {\r\n      alert(UpdataMenu.data);\r\n    } else {\r\n      IsModalOpen(false);\r\n      // window.location.reload();\r\n    }\r\n  };\r\n  useEffect(() => {\r\n    console.log(\"MenuInfo\", MenuInfo);\r\n  }, []);\r\n\r\n  return (\r\n    //make form for edit menu\r\n    <div>\r\n      <form onSubmit={handleSubmit}>\r\n        <div>\r\n          <label>메뉴이름</label>\r\n          <input\r\n            type=\"text\"\r\n            name=\"menu_name\"\r\n            id=\"NewName\"\r\n            defaultValue={MenuInfo.menu_name}\r\n          />\r\n        </div>\r\n        <div>\r\n          <label>가격</label>\r\n          <input\r\n            type=\"number\"\r\n            name=\"menu_price\"\r\n            id=\"NewPrice\"\r\n            defaultValue={MenuInfo.price}\r\n          />\r\n        </div>\r\n        <div>\r\n          <label>메뉴상태</label>\r\n          <select id=\"status\">\r\n            <option value={false} defaultVaule={!MenuInfo.is_forbiddden}>\r\n              판매중\r\n            </option>\r\n            <option value={true} defaultValue={MenuInfo.is_forbidden}>\r\n              판매중지\r\n            </option>\r\n          </select>\r\n        </div>\r\n        <div>\r\n          <label>사진</label>\r\n          <input type=\"text\" value={MenuInfo.image} />\r\n        </div>\r\n        <div>\r\n          <label>옵션</label>\r\n          <select>\r\n            {optionList.map((item) => {\r\n              console.log(\"item\", item);\r\n              console.log(\"optionlist\", optionList);\r\n              return (\r\n                <option value={item.option_name} selected={item.is_forbidden}>\r\n                  {item.option_name}\r\n                </option>\r\n              );\r\n            })}\r\n          </select>\r\n        </div>\r\n        <div>\r\n          <label>옵션가격</label>\r\n          <input type=\"text\" value={MenuInfo.option_price} />\r\n        </div>\r\n        <div>\r\n          <button type=